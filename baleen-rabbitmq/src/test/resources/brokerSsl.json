{
 "name" : "broker",
 "modelVersion" : "6.0",
 "authenticationproviders" : [ {
     "name" : "plain",
     "type" : "Plain",
     "users" : [ {
         "name" : "${qpid.user}",
         "type" : "managed",
         "password" : "${qpid.pass}"
     } ]
 } ],
 "ports" : [ {
     "name" : "AMQP",
     "port" : "${qpid.amqp_port}",
     "authenticationProvider" : "plain",
     "keyStore" : "default",
     "transports" : [ "SSL" ],
     "virtualhostaliases" : [ {
         "name" : "defaultAlias",
         "type" : "defaultAlias"
     }, {
         "name" : "hostnameAlias",
         "type" : "hostnameAlias"
     }, {
         "name" : "nameAlias",
         "type" : "nameAlias"
     } ]
 } ],
 "virtualhostnodes" : [ {
     "name" : "default",
     "type" : "JSON",
     "defaultVirtualHostNode" : "true",
     "virtualHostInitialConfiguration" : "{\"type\" : \"Memory\",\"name\" : \"default\",\"modelVersion\" : \"6.0\"}"
 } ],
 "keystores" : [ {
     "name" : "default",
     "password" : "password",
     "storeUrl": "${qpid.keystore}"
 }]
}